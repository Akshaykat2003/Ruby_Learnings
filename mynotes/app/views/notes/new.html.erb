<%= turbo_frame_tag "note_form" do %>
  <% content_for :title, "New note" %>

  <div class="container">
    <h1>New note</h1>

    <%= render "form", note: @note %>

    <div class="actions">
      <%= link_to "Back to notes", notes_path, "data-turbo-frame": "notes", class: "btn btn-secondary" %>
    </div>
  </div>
<% end %>